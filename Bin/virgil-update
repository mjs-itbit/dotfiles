#!/bin/bash -x

tmutil startbackup -b

sudo softwareupdate -i -a
tmutil startbackup -b

open /Applications/App\ Store.app
read -p "Press Enter when done" FOO
tmutil startbackup -b

brewupdate
tmutil startbackup -b

emacsclient -e '(package-list-packages)'
read -p "Press Enter when done" FOO
tmutil startbackup -b

